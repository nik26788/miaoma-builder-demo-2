{
  "name": "demos-vbuilder",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "eslint --fix",
    "lint:style": "stylelint \"{packages,apps}/**/*.{scss,css,vue}\" --fix",
    "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"(packages|apps)/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@demos-vbuilder/eslint": "workspace:*",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "commitizen": "4.3.1",
    "cspell": "8.17.3",
    "cz-git": "1.11.0",
    "husky": "9.1.7",
    "stylelint": "16.14.1",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "eslint": "9.20.0",
    "turbo": "^2.5.5"
  }
}
